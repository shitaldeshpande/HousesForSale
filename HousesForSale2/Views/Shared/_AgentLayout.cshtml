    <script>

        function btnclick() {

            var agent = {
                FName: $("#inputFirstName_ContactAgentForm").val().trim(),
                LName: $("#inputLastName_ContactAgentForm").val().trim(),
                Email: $("#inputEmail_ContactAgentForm").val().trim(),
                Phone: $("#inputPhone_ContactAgentForm").val().trim(),
                Comment: $("#areaComment_ContactAgentForm").val().trim(),

            }


            alert(JSON.stringify(agent));

            $.ajax({
                type: "POST",
                url: "/api/AgentDetails/AddContact",
                data: agent,
                dataType: "json",
                success: function (data) {
                    setTimeout(function () {
                                        swal({
                                            title: "Thank you!",
                                            text: "Enquiry Send Successfully,Melissa will get back to you very soon.",
                                            type: "success"
                                        }, function () {
                                            window.location = "/House/Agent_Profile";
                                        });
                                    }, 1);
                }, //End of AJAX Success function

                failure: function (data) {
                    alert(data.responseText + 'failuer');
                }, //End of AJAX failure function
                error: function (data) {
                    alert(data.responseText + 'error');
                } //End of AJAX error function

            });

        }



